{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-name": [false],
    "variable-name": [false],
    "quotemark": [true, "single", "jsx-double"],
    "indent": [true, "spaces", 2],
    "prettier": [
      true,
      {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "none",
        "semi": false
      }
    ]
  }
}